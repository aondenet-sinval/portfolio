function testeUm() { 
	a = 'Estou dentro';
	b = 'Estou dentro';

}
testeUm();
console.log('Ainda consigo ver o valor de a: ' + a);
console.log('Ainda consigo ver o valor de b: ' + b);
function testeDois() { 
	var c = 'Estou dentro';
	d = 'beleza então';
}
testeDois();
//console.log('Ainda consigo ver o valor de c: ' + c);
var d = 'Outro valor';
console.log('d é: ',d)

function realizarSoma(a,b) {	
	return a + b;
}
setTimeout(document.onload = console.log(realizarSoma(5,10)), 5000);
